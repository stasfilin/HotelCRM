{
	"name": "HotelCRM",
	"image": "mcr.microsoft.com/devcontainers/universal:2",
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2": {}
	},
	"runArgs": ["--privileged"],
	"mounts": [
	  "type=bind,src=/var/run/docker.sock,dst=/var/run/docker.sock"
	],
	"customizations": {
	  "vscode": {
		"extensions": [
		  "ms-azuretools.vscode-docker"
		]
	  }
	}
  }